{
  // example data.json

  "gameID": "example",
  // spaceDance, spaceball, catchyTune, etc
  "gameName": "Example Game",
  // human-readable name
  "series": "tengoku",
  // the game series this is from
  // can be tengoku, ds, fever, megamix, side
  // if a game is present in Megamix and its original game, use the original game
  // this field is optional for non-canon games

  "cues": [
    // JSON array of cue objects

    {
      "id": "gameFolder/soundFile",
      // where the editor will find the sound file
      "fileExtension": "ogg",
      // OPTIONAL - defaults to ogg
      // please use the ogg vorbis format

      "deprecatedIDs": [],
      // OPTIONAL
      // used internally in case an ID changes

      "duration": 0.5,
      // the duration in beats for this cue - use up to 6 digits (one-third is 0.333333)

      "name": "person - cue - part",
      // human-readable name, ex: gramps - let's sit down - sit

      // OPTIONAL fields below - they are not necessary

      "canAlterPitch": false,
      // if the pitch can be edited (Launch Party, Built to Scale DS, etc.) - default false
      "canAlterDuration": false,
      // if you can stretch/shrink this cue (Moai Doo-Wop, Glee Club, etc.) - default false
      // note that this will set loops to true (unless loops is set manually)

      "introSound": "gameFolder/otherSound",
      // the sound ID used for stuff like Moai Doo-Wop - the "d" sound for ooo
      // note that this sound will automatically be hidden

      "baseBpm": 105,
      // used for Fillbots - the original BPM of the filling sound
      // note that if this is present, canAlterPitch will always be set to false

      "loops": false
      // if the sound loops if the clip is shorter than the cue duration
      // this overrides canAlterDuration's setting
    }
  ],
  "patterns": [
    // JSON array of pattern objects
    // NOTE: this must be present even if patterns aren't used (an empty array [] will work)
    // This can be automatically generated if you enable debug mode with F8, then press R while having cues selected
    // this will export a pattern to the console/log file

    {
      "id": "example_pattern",
      // usually gameID_<name>
      "name": "Pattern 1",
      // human-readable name

      "deprecatedIDs": [],
      // OPTIONAL
      // used internally in case an ID changes

      "isStretchable": false,
      // OPTIONAL if the pattern can be stretched
      // used for stuff like Bouncy Road, Clappy Trio
      // this is automatically true if any of the cues has canAlterDuration set to true

      "cues": [
        {
          "id": "gameFolder/soundFile",
          // the sound ID
          "beat": 0.0,
          // the beat, where 0.0 is the start of the pattern (it's relative)
          "track": 0,
          // the Y-level, 0 is bottom, Y-up

          "duration": 1.0,
          // OPTIONAL - the duration (used for cues with canAlterDuration)
          "semitone": 2
          // OPTIONAL - the semitone difference - positive is higher, negative is lower
          // used for stuff like Bouncy Road where the last two notes change (but are the same sound file)
        }
      ]
    }
  ]
}